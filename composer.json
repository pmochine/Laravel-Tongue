{
  "name": "pmochine/laravel-tongue",
  "description": "Finally a subdomain localization that works how you want it to work.",
  "license": "MIT",
  "keywords": [
    "laravel",
    "tongue",
    "localization",
    "subdomain",
    "i18n"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Philipp Mochine",
      "email": "p@mochine.de"
    }
  ],
  "require": {
    "php": ">=7.0",
    "illuminate/support": "~5.6.0"
  },
  "require-dev": {
    "orchestra/testbench": "~3.6.0",
    "phpunit/phpunit": "~7.0"
  },
  "autoload": {
    "psr-4": {
      "Pmochine\\LaravelTongue\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Pmochine\\LaravelTongue\\Tests\\": "tests"
    },
    "files": [
      "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
    ]
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Pmochine\\LaravelTongue\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
