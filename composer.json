{
  "name": "pmochine/laravel-tongue",
  "description": "üéâ Finally a subdomain localization that works how you want it to work. üåê",
  "license": "MIT",
  "keywords": [
    "laravel",
    "tongue",
    "localization",
    "subdomain",
    "i18n",
    "package",
    "language"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Philipp Mochine",
      "email": "mail@philippmochine.com"
    }
  ],
   "require": {
    "php": "^7.4 || ^8.0",
    "bakame/laravel-domain-parser": "^1.0||^1.1",
    "laravel/framework": "^8.41||^9.0"
  },
  "require-dev": {
    "orchestra/testbench": "^5.0||^6.0||^7.0",
    "orchestra/testbench-browser-kit": "^5.0 ||^6.0||^7.0",
    "phpunit/phpunit": "^9.0||^10.0"
  },
  "autoload": {
    "psr-4": {
      "Pmochine\\LaravelTongue\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Pmochine\\LaravelTongue\\Tests\\": "tests"
    },
    "files": [
      "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
    ]
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Pmochine\\LaravelTongue\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
